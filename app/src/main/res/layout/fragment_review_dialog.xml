<?xml version="1.0" encoding="utf-8"?><layout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/bg_nicknamedialog"
        android:padding="20dp"
        tools:context=".ReviewDialogFragment">

        <View
            android:id="@+id/view17"
            android:layout_width="0dp"
            android:layout_height="1dp"
            android:layout_marginEnd="8dp"
            android:background="@color/green_300"
            app:layout_constraintBottom_toBottomOf="@+id/tv_review_title"
            app:layout_constraintEnd_toStartOf="@+id/tv_review_title"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/tv_review_title" />

        <View
            android:id="@+id/view15"
            android:layout_width="0dp"
            android:layout_height="1dp"
            android:layout_marginStart="8dp"
            android:background="@color/green_300"
            app:layout_constraintBottom_toBottomOf="@+id/tv_review_title"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/tv_review_title"
            app:layout_constraintTop_toTopOf="@+id/tv_review_title" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="후기작성"
            android:textColor="@color/black"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/tv_review_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView3"
            tools:text="텀블러 사용하기" />

        <EditText
            android:id="@+id/et_review"
            android:layout_width="0dp"
            android:layout_height="145dp"
            android:layout_marginTop="8dp"
            android:background="@drawable/et_review"
            android:ems="10"
            android:gravity="start|top"
            android:inputType="textMultiLine"
            android:maxLength="50"
            android:maxLines="5"
            android:padding="8dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_review_title"
            tools:text="자자자내가지금부터오자자자내가지금부\n터오자\n자\n자내가지금부터오자자자내가지금부터오자자자내가지금부터오" />

        <TextView
            android:id="@+id/tv_review_cancle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="16dp"
            android:text="취소"
            app:layout_constraintEnd_toStartOf="@+id/tv_review_ok"
            app:layout_constraintTop_toBottomOf="@+id/et_review" />

        <TextView
            android:id="@+id/tv_review_ok"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="8dp"
            android:text="완료"
            android:textColor="@color/green_300"
            app:layout_constraintBottom_toBottomOf="@+id/tv_review_cancle"
            app:layout_constraintEnd_toEndOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout></layout>
